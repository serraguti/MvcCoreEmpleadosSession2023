@model List<Empleado>

<h1>Session Salarios</h1>

<a asp-controller="Empleados"
    asp-action="SumaSalarios">
    Suma salarial Session
</a>

<ul class="list-group">
    @foreach (Empleado emp in Model)
    {
        <li class="list-group-item">
            @emp.Apellido
            <a asp-controller="Empleados"
                asp-action="SessionSalarios">
                Almacenar salario
            </a>
        </li>
    }
</ul>